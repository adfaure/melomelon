{
    "manifest_version": 3,
    "name": "MeloMelon",
    "version": "0.0",
    "permissions": ["storage", "tabs", "scripting" ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.bundle.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Click to show an alert"
    },
    "icons": {
        "48": "icon48.png",
       "128": "icon128.png"
    }
}