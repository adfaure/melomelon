{
    "manifest_version": 3,
    "name": "MeloMelon",
    "version": "0.0",
    "permissions": ["storage", "tabs" ],
    "host_permissions": [
        "*://*.youtube.com/*"
    ],
    "background": {
        "service_worker": "background.bundle.js"
    },
    "action": {
        "default_popup": "popup.html"
      }
}